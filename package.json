{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js --coverage"
  },
  "jest": {
    "globals": {
      "__testing__": true
    },
    "automock": false,
    "setupFiles": [
      "./jestSetup.js"
    ],
    "preset": "react-native",
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleFileExtensions": [
      "ios.js",
      "native.js",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation|native-base|expo)/"
    ],
    "moduleNameMapper": {
      "styled-components": "<rootDir>/node_modules/styled-components/native/dist/styled-components.native.cjs.js"
    }
  },
  "dependencies": {
    "expo": "^32.0.0",
    "expokit": "^32.1.1",
    "firebase": "^5.9.2",
    "jest": "^24.1.0",
    "jest-expo": "^32.0.0",
    "jest-fetch-mock": "^2.1.1",
    "jsdom": "^14.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-background-timer": "^2.1.1",
    "react-native-google-autocomplete": "^0.1.5",
    "react-native-ionicons": "^4.5.5",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-maps": "^0.23.0",
    "react-native-mock": "^0.3.1",
    "react-native-mock-render": "^0.1.2",
    "react-native-picker-select": "^6.1.0",
    "react-native-sound": "^0.10.12",
    "react-navigation": "^3.3.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-expo": "^5.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest-serializer-enzyme": "^1.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6"
  },
  "private": true,
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
